	<script> 
		var cdnPath = "/wordgirl/cdn_content/games/drtwobrains-html5/";
		loadLib = function() {
			var iPhone = /iPhone/i.test(navigator.userAgent);
			var iPad = /iPad/i.test(navigator.userAgent);
			var webkitNative = /Version/i.test(navigator.userAgent) && !(/Chrome/i.test(navigator.userAgent));
			var iOS = iPhone || iPad;
			var path;
			

			if(iOS || webkitNative) {
				path = cdnPath+ "tools/lib/createjs/libraries_old.js";
			} else {
				path = cdnPath+ "tools/lib/createjs/libraries.js";
			}
			var script = document.createElement('script');
			script.type = 'text/javascript';
			script.src = path;
			
			script.onerror = function() {
				throw(
					'Failed to load library from ' + path
				);
			};
			var head = document.getElementsByTagName('head');
			head[0].appendChild(script);
		};
		loadLib();

	</script>
