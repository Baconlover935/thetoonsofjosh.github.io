    function navWithID( pageID )
    {
        if (pageID == "parents"){
            bridge(links[pageID], 'PBS Parents');
        } else if (pageID == "teachers") {
            bridge(links[pageID], 'PBS Teachers');
        } else if (pageID == "pbskids") {
            window.location = links[pageID];
        } else {
            window.location = "/pbskids2008/wordgirl/" + links[pageID];
        }
    }
    function launchPrintable(targetfile, name, params) {
        var domain = "/enablecdn/pbskids2008/wordgirl/cdn_content/printables/" + targetfile;
        window.open(domain, name, params);
    }

    // kids logo
    var cookieString = "" + document.cookie;
    var kidslogo = "false";
    if ( cookieString.indexOf("pbskids.fromkids") != -1 )
    {
        //alert("from kids cookie found");
        kidslogo = "true";
    } else {
        //alert("kids cookie not found: " + document.cookie);
    }

    // support legacy bookmarks
    function checkLegacy(){
        var pathArray = SWFAddress.getPathNames();
        switch (pathArray[1])
        {
            case "game_1": //freeze frame
                navWithID("freezeframe");
                break;
            case "game_2": //power words
                navWithID("powerwords");
                break;
            case "game_3": //Comic Strip Capers
                navWithID("comicstripcapers");
                break;
            case "game_4": //maze-mania
                navWithID("mazemania");
                break;
            case "game_8": // huggy-dance
                navWithID("huggydance");
                break;
            case "game_9": // robot-workshop
                navWithID("robotworkshop");
                break;
            case "game_10": //build-a-lair
                navWithID("buildalair");
                break;
            case "comicBook": //comic book
                navWithID("comicbook");
                break;
            default:
            // ignore
        }
    }

